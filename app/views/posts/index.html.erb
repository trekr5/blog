

<h>Listing posts</h1>

<%= link_to 'New Post', new_post_path %>
<table>

  <tr>
    <th>Title</th>
    <th>Text</th>

  </tr>

# From the index action in the posts controller the following
# is called for the instance variable @posts

<% @posts.each do |post| %>
  <tr>
    <td><%= post.title %></td>
    <td><%= post.text %></td>
  </tr>
<% end %>

</table>
